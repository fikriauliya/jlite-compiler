        Int dummy1;
        Int dummy2;
        Int dummy3;
        String s1;
        String s2;
        C1 c1;
        C2 c2;
        C3 c3;
        
        dummy1 = 2;
        dummy2 = 3;
        dummy3 = 10;
        
        c1.m1();
