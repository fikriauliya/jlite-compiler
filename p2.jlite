// No variable
class C1
{
    // No parameter
    void m1() {
        println("Hello");
        return;
    }
    
    // 1 parameter
    void m2(Int a) {
        println("Hello");
    }
    
    // More than 1 parameters
    void m3(Int a, Int b) {
        println("Hello");
    }
}

// 1 variable
class C2
{
    String a;
    
    // No parameter
    Int m1() {
        return m2(10);
    }
    
